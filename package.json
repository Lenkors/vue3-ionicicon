{
  "name": "@lenkors/vue3-ionicicon",
  "version": "1.1.3",
  "description": "Vue 3 ionic icons wrapper",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-ionicicon.umd.js",
  "module": "./dist/vue3-ionicicon.es.js",
  "types": "./dist/src/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@Lenkors"
  },
  "exports": {
    ".": {
      "import": "./dist/vue3-ionicicon.es.js",
      "require": "./dist/vue3-ionicicon.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "fast-glob": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.8.5"
  },
  "keywords": [
    "icon pack",
    "icon",
    "svg",
    "mobile",
    "vue3",
    "vue",
    "web component",
    "component",
    "custom element",
    "material design",
    "ios"
  ],
  "homepage": "http://ionicons.com/",
  "author": {
    "name": "Alex Zhukovskiy",
    "web": "https://github.com/Lenkors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lenkors/vue3-ionicicon.git"
  },
  "bugs": {
    "url": "https://github.com/Lenkors/vue3-ionicicon/issues"
  },
  "license": "MIT"
}
